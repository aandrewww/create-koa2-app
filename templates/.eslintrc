{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "plugins": ["prettier"],
  "rules": {
    "no-debugger": 1,
    "comma-dangle": ["error", { "functions": "never" }],
    "no-await-in-loop": 0,
    "no-confusing-arrow": 0,
    "no-mixed-operators": 0,
    "no-underscore-dangle": 1,
    "class-methods-use-this": 0,
    "max-len": ["error", 140, 2, { "ignoreStrings": true }]
  },
  "env": {
    "es6": true,
    "node": true
  }
}
